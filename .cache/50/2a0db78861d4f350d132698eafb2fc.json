"original":{"line":28,"column":19}},{"name":"Home","generated":{"line":121,"column":15},"source":"scripts/pages/home.js","original":{"line":75,"column":15}}],"sources":{"scripts/pages/home.js":"import View from './view'\nimport {\n  repeat\n} from '../helper/utils'\nimport {\n  tmdb\n} from '../services/api'\n\nconst template = `\n  <div class=\"nc-track\">\n    <h2 class=\"nc-title\">신규 콘텐츠</h2>\n    <div class=\"swiper-container\">\n      <div class=\"swiper-wrapper\" ref=\"popular\">\n        ${repeat('<div class=\"swiper-slide\"><a href=\"/\"></a></div>', 12)}\n      </div>\n    </div>\n  </div>\n  <div class=\"nc-track\">\n    <h2 class=\"nc-title\">지금 뜨는 콘텐츠</h2>\n    <div class=\"swiper-container\">\n      <div class=\"swiper-wrapper\">\n        ${repeat('<div class=\"swiper-slide\"><a href=\"/\"></a></div>', 12)}\n      </div>\n    </div>\n  </div>\n`\n\nclass Home extends View {\n  constructor (router) {\n    super({\n      innerHTML: template,\n      className: 'nc-home'\n    })\n\n    this._router = router\n  }\n\n  mounted () {\n    this.initPopular()\n  }\n\n  destroyed () {\n\n  }\n\n  async initPopular () {\n    const { results } = await tmdb.getMoviePopular()\n    const popular = this.$refs.popular\n    while (popular.hasChildNodes()) {\n      popular.removeChild(popular.lastChild)\n    }\n\n    for (let i = 0; i < results.length; i++) {\n      const data = results[i]\n      const isLast = i === results.length - 1\n      requestAnimationFrame(() => {\n        this.$refs.popular.insertAdjacentHTML('beforeend', `\n          <div class=\"swiper-slide\">\n            <a href=\"/\">\n              <img data-src=\"${tmdb.IMG_URL + data.backdrop_path}\">\n            </a>\n          </div>\n        `)\n\n        if (isLast) {\n          const images = this.$refs.popular.querySelectorAll('[data-src]')\n          this.lazyLoad(images)\n        }\n      })\n    }\n\n  }\n}\n\nexport default Home"},"lineCount":null}},"error":null,"hash":"92c84772710403bc7e2386afeeb561c8","cacheData":{"env":{}}}